<odoo>
    <template id="article_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <tr>
                    <td><span t-field="doc.sequence_number"/></td>
                    <td><span t-field="doc.title"/></td>
                    <td><span t-field="doc.publish_date"/></td>
                    <td><span t-field="doc.assigned_to_id.name"/></td>
                    <td><span t-field="doc.finished_date"/></td>
                </tr>
            </t>
        </t>
    </template>
</odoo>

